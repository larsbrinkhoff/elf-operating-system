	.NLIST			;NO MACRO DEFS PLEASE
	.MACRO	$SNAME
	.ASCII	"SCRL ELF SYSTEM 75C.002.1"
	.ENDM	$SNAME

	.MACRO	$CNFIG		;SYSTEM PROCESSOR CONFIGURATION
CRUNCH	=	1		;SACRIFICE FEATURES TO REDUCE SIZE
DEBUG	=	0		; ENABLE THE DEBUG OPTION
P1145	=	0		;SET PDP 11/45 PROCESSOR
P1140	=	0		;NOT AN 11/40
P1120	=	1		;SET 11/20 PROCESSOR
P1105	=	0		;NOT AN 11/05
P1110	=	0		;NOT AN 11/10
PEAE	=	0		;NO EAE
PMDV	=	0		; MULTIPLY/DIV OPTION ON 11/35 /40
PMMGMT	=	0		;MEMORY MANAGEMENT OPTION
PFLTPT	=	0		;NO FLT POINT OPTION
PAGING	=	0		;PAGING PARAMETER

	.IF NE P1140+P1145+P1120+P1110+P1105-1
	.ERROR	P1145	;PROCESSOR SPECIFICATION ERROR
	.ENDC
	.IF EQ	P1105!P1110!P1120!P1140!P1145
	.ERROR	1	;PROCESSOR SPECIFICATION ERROR
	.ENDC
	.IF NE P1120!P1110!P1105&PMMGMT
	.ERROR	PMMGMT	;MEMORY MANAGEMENT ERROR
	.ENDC
	.ENDM


	.MACRO	$KPARM		;KERNEL PARAMETERS


;		DEFINE KERNEL CONFIGURATION
;			PARAMETERS

ELFPRI	=	7		;PRIORITY OF ELF WHEN DISABLED
NPRQ	=	6.		;NUMBER OF PROCESS READY QUEUES
NKEMT	=	64.		;NUMBER OF KERNEL EMT POINTERS
NUEMT	=	12.		;NUMBER OF USER EMT'S
NTQE	=	16.		;NUMBER OF TIMER QUEUE ELEMENTS
NPCT	=	29.		;MAX NUMBER OF PCTS (PROCESSES)
NPST	=	32.		; (128K) MAX NUMBER OF MEM PAGES
NVSM	=	6.		;NUMBER OF VIRTUAL STORAGE MAPS
NPCTL	=	96.		;PCT LENGTH, WORDS.
NVASIZ	=	32.		;NUMBER BYTES VS ALLOCATION BLOCK
NPMQE	=	64.		;NUMBER OF PROCESS MQE'S
NSEMS	=	32.		;NUMBER OF SEMAPHORES
NFSEMS	=	NPCT		;NUMBER OF FREE SEMAPH. QUEUE ELS.
NIOQEL	=	32.		;NUMBER OF I/O REQUEST QUEUE ELEMENTS
NFUSLS	=	NPCT-4		; NUMBER OF USER STACK LIST ENTRIES


	.IF EQ	PMMGMT		;NO MEMORY MANAGEMENT OPTION?
NPST	=	10		;THEN RE-DEFINE VIR STORAGE PARAMS
NVSM	=	1		;ONLY 1 STORAGE MAP (KERNEL)
	.ENDC
	.ENDM	$KPARM
	.MACRO	$IOCON

;	-------  Terminal Device Type Specification  -------

;	    Device driver support is generated in module KKB
;	    for the following devices if their name is equated
;	    to 1:

TTY	= 1		; Teletype (DL11 or KL11)
TSB	= 1		; SCRL Time Sharing Bus
DJ11	= 0		; DJ11 Line Multiplexor
DH11	= 0		; DH11/DM11 LINE/MODEM MULTIPLEXER

;	-------  IMP INTERFACE SPECIFICATION  -------

;	POSSIBLE INTERFACES:

I.VDH	= 1		;  VERY DISTANT HOST
I.ISI	= 2		;  ISI VARIANT OF ANTS INTERFACE
I.ANTS	= 3		;  STRAIGHT ANTS INTERFACE

I.TYPE	= I.ISI		; <--- INTERFACE IS BOB PARKER SPECIAL.


I.HMRI	= 1000.		; Time interval allowed between settings
			; of Host Master Ready line. ... Time is
			; in milliseconds.

LP	=	1	;DEC LINE PRINTER, LS11

	.ENDM	$IOCON

	.MACRO	$KCON			; Kernel Configuration

;	    Optimization Option:

K.TIME	= 1			; Minimize processing time
K.CORE	= 2			; Minimize storage requirement

K.OPT	= K.TIME	; <--- Optimize time.

;	--- Boolean versions of the Opt parameter:

K.OPTT	= K.OPT-K.TIME+1	; 1 => Optimize for time
K.OPTC	= 1-K.OPTT		; 1 => Optimize for core

	.ENDM	$KCON


	.MACRO $DFREG
R0=%0
R1=%1
R2=%2
R3=%3
R4=%4
R5=%5
R6=%6
SP=%6
PC=%7
PS=^O177776		;PROCESSOR STATUS
	.IF NE	P1140!P1145&PMMGMT	;MEMORY MANAGEMENT?
KDR0	=	^O172300	;KERNEL DESCRIPTOR REG 0 (I-SPACE)
KAR0	=	^O172340	;KERNEL ADDRESS REG 0
UDR0	=	^O177600	;USER DESCRIPTOR REG 0 (I-SPACE)
UAR0	=	^O177640	;USER ADDRESS REG 0

SR0	=	^O177572	;SEGMENTATION REG 0
SR1	=	^O177574	;SEG REG	1
SR2	=	^O177576	;SEG REG	2
	.IF EQ	P1145&PMMGMT-1
SR3	=	^O172516	;SEG STAT REG 3  11/45 ONLY
	.ENDC
	.ENDC
	.IF EQ	PEAE-1		;EAE DEFINED
AC = ^O177302			;EAE -- AC
MQ = ^O177304			;EAE -- MQ
XMPY = ^O177306			;MULTIPLICAND
XSTAT = ^O177310		;EAE -- SC,STATUS BYTES
XDVSR = ^O177300		;EAE -- DIVISOR REGISTER
LSHFT = ^O177314		;EAE -- LOGICAL SHIFT
ASHFT = ^O177316		;EAE -- ARITHMETIC SHIFT
	.ENDC
	.ENDM


	.MACRO	$IOTBL

;			ELF SYSTEM I/O CONFIGURATION


	.PAGE
	.SBTTL	DEFINE THE CONFIGURATION
.

;			THIS MACRO DEFINES THE I/O
;			CONFIGURATION FOR AN ELF SYSTEM.

;			IT IS EXPANDED BY THE KDCT MODULE
;			DURING ELF GENERATION.

;THE FOLLOWING MACROS DEFINE THE CONFIGURATION AT SCRL

;DEFINITION OF GENERIC DEVICES

	DEFDEV	<DF,DK,TTY,LP,MT,CLK,DT,IPP,VDH,DRB>

;INTERRUPT CONTROL TABLES

;	ICT	VCTR,PRIO,INT HANDLER,CSR,INIT

;	LINE PRINTER ICT
	ICT	200,ELFPRI,$LPIN,177514,$LPINT

;DR11B DEVICE INTERFACE
	ICT	124,ELFPRI,$DRINT,172414,$DRINI

;CONSOLE TTY
	ICT	60,ELFPRI,$TII,177560,$ITTY
	ICT	64,ELFPRI,$TOI,177564,$ITTY

;TIME-SHARING BUS

	ICT	320,ELFPRI,$TSBI,167620,$ITSB


;DL-11 INTERFACE

	ICT	300,ELFPRI,$TII,175610,$ITTY
	ICT	304,ELFPRI,$TOI,175614,$ITTY

;	ARPANET VERY-DISTANT HOST INTERFACE

	ICT	270,ELFPRI,$VIINT,167600,$VIINI
	ICT	274,ELFPRI,$VOINT,167610,$VOINI

;DEVICE CONTROL TABLES AND EXTENSIONS

;	DCT	DEV,UNIT,AUXILARY,DEVREG,INTLST,WRKSIZ,[O]

;LINE PRINTER
	DCT	LP,0,177514,<200>,1
	IODEV	100000,204,$LPTF
	CHAR

;DR11B DCT'S  TWO ARE REQUIRED
;	ONE FOR REGULAR I/O
;	SECOND ONE FOR POLLING STATUS.
	DCT	DRB,0,172414,<124>
	IODEV	0,10000,$DRTF
	BLOCK	10000,0,0,10000

	DCT	DRB,1,172414,<124>
	IODEV	0,10000,$DRTF
	BLOCK	10000,0,0,10000

;CONSOLE TTY
	DCT	TTY,0,177562,<60>
	IODEV	0,8192.,$TTIN
	CHAR	20,TTY0O		;RING BUFFER OF 20 CHARS

	DCT	TTY,0,177566,<64>,,O
	IODEV	100000,8192.,$TTOUT
	CHAR	,TTY0		;NO RING BUFFER, MATE DCT

;TIME-SHARING BUS:  TTY'S 1-16

	.IRP	TTYNO,<1,2,3,4,5,6,7,10,11,12,13,14,15,16,17,20>

	DCT	TTY,TTYNO,167402-10+<TTYNO*10>,<320>
	IODEV	0,8192.,$TSBIN
	CHAR	20,TTY'TTYNO'O

	DCT	TTY,TTYNO,167406-10+<TTYNO*10>,<320>,,O
	IODEV	100000,8192.,$TSBOU
	CHAR	,TTY'TTYNO

	.ENDR

;DL-11 INTERFACE

	DCT	TTY,21,175612,<300>
	IODEV	0,8192.,$TTIN
	CHAR	20,TTY21O		;20 CHAR. RING BUFFER, MATE DCT

	DCT	TTY,21,175616,<304>,,O
	IODEV	100000,8192.,$TTOUT
	CHAR	,TTY21			;NO RING BUFFER, MATE DCT

;		ARPANET VERY-DISTANT HOST INTERFACE

	DCT	VDH,0,167600,<270>
	IODEV	0,128.,$VITRF
	BLOCK	128.,0,10000,128.

	DCT	VDH,1,167600,<270>
	IODEV	4,128.,$VITRF
	BLOCK	128.,0,10000,128.

	DCT	VDH,2,167610,<274>
	IODEV	0,128.,$VOTRF
	BLOCK	128.,0,10000,128.
	.PAGE
	.SBTTL	INTER-PROCESS PORTS


NRPORTS	=	16.	;NUMBER OF INTER-PROCESS PORTS
PORTN	=	0		;PORT NUMBER INDEX
	.REPT	NRPORTS		;GENERATE INTER-PROCESS PORTS
	DCT	IPP,\PORTN,0
	PORT
PORTN	=	PORTN+1		;INCREMENT PORT NUMBER
	.ENDR
	.ENDM	$IOTBL

	.MACRO	$FLCNF
;THE FOLLOWING CONTROL THE ASSEMBLY OF FLEA:
FILL	=	0			;FILL COMMANDS (;F, ;I)
RELOC	=	0			;RELOCATION COMMANDS (R, !)
SEARCH	=	0			;SEARCH COMMANDS (;W, ;E)
OFFSET	=	0			;OFFSET COMMAND (;O)
RAD50	=	0			;RADIX-50 COMMAND (X)
BRANCH	=	0			;BRANCH COMMANDS (>, _)
RETURN	=	0			;RETURN COMMAND (<)
INDRCT	=	0			;INDIRECT COMMAND (@)
BREAK	=	0			;BREAK POINT COMMANDS
					;   (;B, ;T, ;G, ;J, ;K, ;Z, ;P, ;$)
SPCLOC	=	0			;SPECIAL LOCATION COMMAND ($)
SEMCLN	=	0			;SEMICOLON COMMAND (;)
PLSMNS	=	0			;PLUS AND MINUS COMMANDS (+, -)
CONREG	=	0			;CONSTANT REGISTERS
RELREG	=	0			;RELOCATION REGISTERS
MMREG	=	0			;MEMORY MANAGEMENT REGISTERS
UVAREG	=	0			;USER VIRTUAL ADDRESS REGISTERS
OPNADS	=	0			;OPEN ADDRESS SPACE COMMAND (&)
FAVPN	=	6			;FLEA AVAILABLE VIRTUAL PAGE NUMBER
	.ENDM	$FLCNF
;			NCP CONFIGURATION SPECIFICATIONS

	.MACRO	$NPARM		;SET NCP PARAMETERS

N.XNCP	=	0		; XNCP OPTION
XLNK	=	300		;  MIN LINK NUMBER FOR XNCP
NINTVL	=	800.		; NUMBER OF MSECS. BETWEEN RESETS
				; RESET MESSAGES.
HADDR	=	166		; THIS HOST ADDR (OCTAL)
N.ISIZ	=	64.		; INTERMEDIATE BUFFER SIZE, BYTES
N.FQEL	=	48.		; NUMBER OF MESSAGE QUEUE ELEMENTS

	.ENDM	$NPARM

	.MACRO	$NPOOLS
;			POOL ID VALUES. (I.E., POOL 0 IS THE
;			FIRST BUFFER POOL DEFINED IN THE LIST.)

	$POOL	5.,128.
	$POOL	2,1024.
	$POOL	24.,64.	; THESE ARE THE MESSAGE BUFFS
	$POOL	16.,32.
	$POOL	32.,4
	$POOL	8.,8.
	$POOL	32.,16.	; 32 16-BYTE ENTRIES.
	.ENDM

